{
  "name": "secure-package-manager-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "python:lint": "cd .. && python -m flake8 backend/ tests/",
    "python:format": "cd .. && python -m black backend/ tests/",
    "python:imports": "cd .. && python -m isort backend/ tests/",
    "python:typecheck": "cd .. && python -m mypy backend/",
    "python:all": "npm run python:imports && npm run python:format && npm run python:lint && npm run python:typecheck"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-date-pickers": "^8.11.0",
    "@tanstack/react-table": "^8.21.3",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "material-react-table": "^2.13.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.8.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "@vitejs/plugin-react": "^5.0.3",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "prettier": "^3.6.2",
    "typescript": "^4.9.3",
    "vite": "^7.1.7"
  }
}
